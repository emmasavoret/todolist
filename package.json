{
  "name": "todolist",
  "version": "0.0.1",
  "description": "to do list",
  "main": "index.js",
  "scripts": {
    "start": "tsc && node index.js",
    "dev": "tsc && concurrently --kill-others \"tsc -w\" \"nodemon index.js --signal SIGKILL\"",
    "serve": "nodemon index.js --signal SIGKILL",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emmasavoret/todolist.git"
  },
  "author": "Emma Savoret",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/emmasavoret/todolist/issues"
  },
  "homepage": "https://github.com/emmasavoret/todolist#readme",
  "dependencies": {
    "express": "^4.16.4"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "concurrently": "^4.0.1",
    "nodemon": "^1.18.4",
    "typescript": "^3.1.3"
  }
}
